TxScript TransferAlph(
    tokenBridge: TokenBridge,
    fromAddress: Address,
    toChainId: U256,
    toAddress: ByteVec,
    alphAmount: U256,
    messageFee: U256,
    arbiterFee: U256,
    nonce: ByteVec,
    consistencyLevel: U256
) {
    tokenBridge.transferAlph{
        fromAddress -> messageFee + alphAmount
    }(
        fromAddress,
        toChainId,
        toAddress,
        alphAmount,
        arbiterFee,
        nonce,
        consistencyLevel
    )
}
