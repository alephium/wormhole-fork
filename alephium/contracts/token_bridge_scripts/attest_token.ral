TxScript AttestToken(
    payer: Address,
    tokenBridge: TokenBridge,
    localTokenId: ByteVec,
    messageFee: U256,
    nonce: ByteVec,
    consistencyLevel: U256
) {
    tokenBridge.attestToken{
        payer -> ALPH: messageFee, localTokenId: 1
    }(payer, localTokenId, nonce, consistencyLevel)
}
