import "../token_bridge/token_bridge.ral"

TxScript Main(payer: Address, tokenBridgeId: ByteVec, localTokenId: ByteVec, alphAmount: U256) {
    let tokenBridge = TokenBridge(tokenBridgeId)
    tokenBridge.createLocalTokenPool{
        payer -> alphAmount, localTokenId: 1
    }(localTokenId, payer, alphAmount)
}
