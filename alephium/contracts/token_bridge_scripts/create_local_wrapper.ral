import "../token_bridge/token_bridge_for_chain.ral"

TxScript Main(payer: Address, tokenBridgeForChainId: ByteVec, localTokenId: ByteVec, alphAmount: U256) {
    let tokenBridgeForChain = TokenBridgeForChain(tokenBridgeForChainId)
    tokenBridgeForChain.createWrapperForLocalToken{
        payer -> alphAmount, localTokenId: 1
    }(localTokenId, payer, alphAmount)
}
