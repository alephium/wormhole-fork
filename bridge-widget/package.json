{
  "name": "@alephium/bridge-widget",
  "version": "0.0.3",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./hosted": {
      "types": "./lib/hosted.d.ts",
      "require": "./lib/hosted.js",
      "import": "./lib/hosted.mjs"
    }
  },
  "files": [
    "lib",
    "dist"
  ],
  "dependencies": {
    "@alephium/token-list": "0.0.12",
    "@alephium/walletconnect-ethereum-provider": "0.2.0",
    "@alephium/walletconnect-qrcode-modal": "0.1.0",
    "@alephium/web3": "^2.0.7",
    "@alephium/web3-react": "^2.0.7",
    "@alephium/wormhole-sdk": "0.6.0-rc.1",
    "@metamask/detect-provider": "^1.2.0",
    "@randlabs/myalgo-connect": "^1.1.3",
    "@project-serum/serum": "^0.13.60",
    "@reduxjs/toolkit": "^1.6.1",
    "@solana/spl-token": "^0.1.6",
    "@solana/spl-token-registry": "^0.2.216",
    "@solana/wallet-adapter-base": "^0.9.3",
    "@solana/wallet-adapter-react": "^0.15.3",
    "@solana/wallet-adapter-react-ui": "^0.9.5",
    "@solana/wallet-adapter-wallets": "^0.16.1",
    "@solana/web3.js": "^1.35.1",
    "@wormhole-foundation/sdk": "3.11.0",
    "algosdk": "2.4.0",
    "axios": "1.11.0",
    "bn.js": "^5.1.3",
    "borsh": "^0.4.0",
    "bs58": "^5.0.0",
    "clsx": "^1.1.1",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "ethereum-multicall": "^2.17.0",
    "ethers": "^5.7.1",
    "js-base64": "^3.6.1",
    "luxon": "^2.3.1",
    "notistack": "^2.0.8",
    "numeral": "^2.0.6",
    "qrcode.react": "^3.0.1",
    "react-confetti-explosion": "^3.0.3",
    "react-i18next": "^14.1.2",
    "react-modal": "^3.15.1",
    "react-redux": "^7.2.4",
    "redux": "^3.7.2",
    "swr": "^2.1.5",
    "use-debounce": "^7.0.0"
  },
  "scripts": {
    "prestart": "npm run build:common",
    "start": "NODE_OPTIONS=--max-http-header-size=32000 REACT_APP_WIDGET_ENV=Testnet vite --host --force",
    "start:mainnet": "NODE_OPTIONS=--max-http-header-size=32000 REACT_APP_WIDGET_ENV=Mainnet vite --host --force",
    "build:common": "cd ../bridge-common && npm run build",
    "prebuild": "npm run build:common",
    "build": "npm run build:lib; npm run build:hosted",
    "build:lib": "NODE_ENV=production NODE_OPTIONS=--max-old-space-size=10240 vite build",
    "build:minimal": "VITE_BUILD_MINIMAL=1 NODE_ENV=production NODE_OPTIONS=--max-old-space-size=10240 vite build",
    "build:hosted": "VITE_BUILD_HOSTED=1 NODE_ENV=production NODE_OPTIONS=--max-old-space-size=10240 vite build",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier": "prettier --write ./src",
    "prettier:check": "prettier --check ./src",
    "analyze": "ANALYZE=true NODE_ENV=production NODE_OPTIONS=--max-old-space-size=10240 vite build",
    "preview": "vite preview",
    "unused:exports": "ts-unused-exports ./tsconfig.json --ignoreLocallyUsed",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:with-wallet": "playwright test --grep-invert @noWallet",
    "test:e2e:without-wallet": "playwright test --grep @noWallet"
  },
  "browserslist": {
    "production": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 3 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.4",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.32.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/styles": "^6.4.8",
    "@mui/styled-engine": "^7.3.5",
    "@mui/system": "^7.3.5",
    "@playwright/test": "1.54.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^16.4.7",
    "env-cmd": "^10.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.11",
    "prettier": "^2.8.8",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "tss-react": "^4.9.19",
    "ts-unused-exports": "^10.1.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.38.0",
    "vite": "7.1.5",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/styles": "^6.4.8",
    "@mui/system": "^7.3.5",
    "@mui/styled-engine": "^7.3.5",
    "tss-react": "^4.9.19",
    "@types/react": ">=18.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "lint-staged": {
    "!(*.js|*.ts|*.tsx|*.jsx|*.cjs|*.mjs)": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx,jsx,cjs,mjs}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "overrides": {
    "axios": "1.11.0",
    "vite-plugin-dts": {
      "vue-tsc": "2.0.0"
    },
    "@solana/wallet-adapter-base": {
      "react": ">=18.0.0",
      "react-dom": ">=18.0.0"
    },
    "@keystonehq/sdk": {
      "react": ">=18.0.0",
      "react-dom": ">=18.0.0"
    },
    "@fractalwagmi/solana-wallet-adapter": {
      "react": ">=18.0.0",
      "react-dom": ">=18.0.0"
    },
    "@binance/w3w-qrcode-modal": {
      "react": ">=18.0.0",
      "react-dom": ">=18.0.0"
    },
    "@apollo/client": {
      "react": ">=18.0.0",
      "react-dom": ">=18.0.0"
    },
    "@web3modal/core": {
      "react": ">=18.0.0",
      "react-dom": ">=18.0.0"
    }
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
