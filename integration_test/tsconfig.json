{
  "include": ["alph.ts", "eth.ts", "utils.ts", "it.ts"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "target": "es2020",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "declaration": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}