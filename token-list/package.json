{
  "name": "token-list",
  "version": "0.0.0",
  "license": "GPL",
  "type": "commonjs",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "rm -rf dist && npx tsc --build .",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --fix --ext ts",
    "jest": "jest --useStderr --silent=false --verbose=true --config jestconfig.json",
    "test": "npm run jest"
